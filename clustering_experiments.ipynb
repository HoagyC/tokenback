        "# calculate the Euclidean distances from mean_vector to all the rows\n",
        "distances = torch.norm(embeddings - vocab_centroid, dim=1)\n",
        " \n",
        "# get the indices of the sorted distances\n",
        "_, indices = torch.sort(distances)\n",
