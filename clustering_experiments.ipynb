     min_cluster = None\n","            for i in clusters[c]:\n","                print('Working on cluster ', c, ' size =', len(clusters[c]), '; token embedding: ', i)\n","                point = embeddings[i]\n","                for j in range(k):\n","                    if j == c:\n","                        continue\n","                    centroid = kmeans.cluster_centers_[j]\n","                    distance = torch.norm(point.to(device) - torch.from_numpy(centroid).to(device))\n","                    if distance < min_distance:\n","                        min_distance = distance\n","                        min_point = i\n","                        min_cluster = j\n","            # Move the point to the closest cluster\n","            clusters[c].remove(min_point)\n","            clusters[min_cluster].append(min_point)\n","            new_assignments[min_point] = min_cluster\n","\n","    # Get the new cluster assignments\n","    kmeans.labels_ = new_assignments\n","\n","    # Get the centroids of each cluster\n","    centroids = kmeans.cluster_centers_\n","    centroids = torch.from_numpy(centroids)\n","\n","    # return the centroids"],"metadata":{"id":"FGoMbERVewnd","executionInfo":{"status":"ok","timestamp":1673954174899,"user_tz":0,"elapsed":413,"user":{"displayName":"The Inamorata Press","userId":"11358823481707850507"}}},"execution_count":8,"outputs":[]},{"cell_type":"code","source":["def generate(model, input, output_length):\n","  ix = tokenizer.encode(input)\n","  print('{} input tokens: {}'.format(len(ix), [tokenizer.decode(i) for i in ix]))\n","  model_out = model.generate(torch.tensor(ix).unsqueeze(0).to(device), max_length = output_length + len(ix))\n","  dec_out = tokenizer.decode(model_out[0])\n","  #print('\\nOutput:\\n{}'.format(dec_out))\n","  return dec_out\n","  # pushes input string throught GPT2 (or whichever model) iteratively producing output_len number of tokens, then prints input + output."],"metadata":{"id":"F9OF4Uocj7LY","executionIn